package com.dbex;

import java.sql.*;

import buf.cio;

public class ConnUtil {
	Connection con = null; //연결위치?
	Statement stmt = null;
	
	try {
		
	} catch (ClassNotFoundException cn) {
		cn.printStackTrace();
	} catch (SQLException s) {
		s.printStackTrace();
	} finally {
		try {
			if(stmt != null) {
				stmt.close();
			}
		} catch (SQLException se) {}
		try {
			if(con != null) {
				con.close();
			}
		} catch (SQLException se) {}
	}
}
